# AWS S3 Configuration for Poneglyph MapReduce
# Copy this file to .env.s3.local and fill in your actual AWS credentials

# AWS Credentials (get these from your AWS Academy account)
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-access-key-here
AWS_SESSION_TOKEN=your-session-token-here

# S3 Configuration
AWS_S3_BUCKET=poneglyph
AWS_S3_BASE_PATH=results/
AWS_REGION=us-east-1

# Optional: For debugging
AWS_SDK_LOAD_CONFIG=true